                labels: aLabels,
                datasets: [{
                    label: "Test strips used",
                    data: aDatasets1,
                    fill: false,
                    borderColor: "#446E7A",
                    backgroundColor: "rgba(141, 177, 171, .4)",
                    pointBackgroundColor: "#CEE397",
                    pointBorderColor: "#070342",
                    pointHoverBackgroundColor: "#CEE397",
                    pointHoverBorderColor: "#CEE397",
                    borderWidth: 3
