            var aData = chData;
            var aLabels = aData[0];
            var aDatasets1 = aData[1];
            var dataT = {
                labels: aLabels,
                datasets: [{
                    label: "Test strips used",
                    data: aDatasets1,
                    fill: false,
                    borderColor: "#FF2C14",
                    borderDash: [5, 5],
                    backgroundColor: "#e755ba",
                    pointBackgroundColor: "#FFBD19",
                    pointBorderColor: "#FFBD19",
                    pointHoverBackgroundColor: "#FFBD19",
                    pointHoverBorderColor: "#FFBD19",
                    borderWidth: 3
